{
  "name": "simple-recordrtc-library",
  "version": "1.0.0",
  "description": "A modular audio recording and waveform visualization library based on RecordRTC",
  "type": "module",
  "main": "public/assets/js/lib/index.js",
  "exports": {
    ".": {
      "import": "./public/assets/js/lib/index.js",
      "require": "./public/assets/js/lib/index.cjs"
    },
    "./LiveWaveform": {
      "import": "./public/assets/js/lib/LiveWaveform.js"
    },
    "./AccumulatedWaveform": {
      "import": "./public/assets/js/lib/AccumulatedWaveform.js"
    },
    "./RecordingManager": {
      "import": "./public/assets/js/lib/RecordingManager.js"
    },
    "./AudioProcessor": {
